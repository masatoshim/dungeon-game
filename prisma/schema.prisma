generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "sqlite"
}


model Dungeon {
  id           String   @id @default(uuid())
  name         String
  description  String?  
  // マップデータはJSON形式の文字列として保存
  // Todo:PostgreSQL移行時は Json 型に
  mapData      String   
  timeLimit    Int      @default(60)
  difficulty   Int      @default(1)  
  createdBy    String
  updatedBy    String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
